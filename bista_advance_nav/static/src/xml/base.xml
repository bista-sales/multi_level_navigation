<?xml version="1.0" encoding="UTF-8"?>
<template>
<t t-name="WebClient">
    <div class="openerp openerp_webclient_container">
    <div class="oe_webclient">	
	<div style="display: table;width: 100%;">
            <div class="oe_topbar">
		<button class="oe_leftbar_open show">open</button>
                <div class="oe_menu_placeholder"/>
                <div class="oe_user_menu_placeholder"/>
                <div class="oe_systray"/>		
            </div>
        </div>
	<div style="display: table; height: 100%; width: 100%;">		
            <div class="oe_leftbar" valign="top" style="display: table-cell;">
                <t t-set="debug" t-value="__debug__ ? '&amp;debug' : ''"/>		
                <a class="oe_logo" t-attf-href="/?ts=#{Date.now()}#{debug}">
                    <span class="oe_logo_edit">Edit Company data</span>
                    <img t-att-src='widget.session.url("/web/binary/company_logo")'/>
                </a>
		<div class="oe_secondary_menus_container"/>
                <div class="oe_footer">
                    Powered by <a href="http://wv8.wirelessvision.com" target="_blank"><span>Wireless Vision</span></a>
                </div>
            </div>		
            <div class="oe_application" style="display: table-cell;">
				
            </div>
        </div>
    </div>
    </div>
</t>
</template>
